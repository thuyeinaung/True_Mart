<?xml version="1.0" encoding="utf-8"?>
<odoo>
		<!--  PDF Format for display debit note -->
        <record id="paperformat_productlabel" model="report.paperformat">
		    <field name="name">Product Barcode Label Format</field>
		    <field name="default" eval="True" />
		    <field name="format">custom</field>
		    <field name="page_height">51</field>
		    <field name="page_width">114</field>
		    <field name="orientation">Portrait</field>
		    <field name="margin_top">5</field>
		    <field name="margin_bottom">10</field>
		    <field name="margin_left">1</field>
		    <field name="margin_right">1</field>
		    <field name="header_line" eval="False" />
		    <field name="header_spacing">0</field>
		    <field name="dpi">90</field>
		</record>
		
</odoo>